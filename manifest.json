{
  "manifest_version": 2,

  "name": "shared-link-for-chrome-extension",
  "description": "Chrome extension such as Shared Links in the Safari",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
